// a library to wrap and simplify api calls
export const create = (api) => ({
  post<%=props.name%>: data => api.post('<%=props.lowerName%>/create', { data }),
  get<%=props.name%>: data => api.get('<%=props.lowerName%>/read' + data._id ),
  get<%=props.name%>s: data => api.get('<%=props.lowerName%>/read'),
  update<%=props.name%>: data => api.patch('<%=props.lowerName%>/update', { data }),
  remove<%=props.name%>: data => api.delete('<%=props.lowerName%>/' + data._id ),
})